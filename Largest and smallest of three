a=int(input())
b=int(input())
c=int(input())

if a>b and a>c:
    print(a,"is greater")
    if b>c:
        print(c,"is the smallest")
    else:
        print(b,"is the smallest")

elif b>a and b>c:
    print(b,"is greatest")
    if a>c:
        print(c,"is smaller")
    else:
        print(a,"is smaller")
    
else:
    print(c,"is greatest")
    if a>b:
        print(b,"is smaller")
    else:
        print(a,"is smaller")
